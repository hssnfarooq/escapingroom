<script>
$(document).ready(function() {
     
		    $(".fa-search").click(function() {
		       $(".search-box").toggle(1000);
		       $("input[type='text']").focus(1000);
		     });
 
 		});

</script> 
<script>
$(document).ready(function() {
$('#go-to-top').each(function(){
    $(this).click(function(){ 
        $('html,body').animate({ scrollTop: 0 }, 3000);
        return false; 
    });
	 })
 })
    </script> 
<script>
         jQuery(window).scroll(function() {

             if (jQuery(this).scrollTop() > 1){  
                 jQuery('.nav_holder').addClass("sticky slideInDown");                   
             }
             else{
                 jQuery('.nav_holder').removeClass("sticky  slideInDown");			   

             }

         });         

      </script> 
